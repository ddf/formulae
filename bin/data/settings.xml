  <program name="trance+form" bits="8" tempo="32" memory="64">
    <code>
      <![CDATA[
  t==0 ? p = 0;
      
  @0 = T(q/16) % 7; 		  
  @1 = T(q/32) % 3;
  @2 = 1 + T(q/128) % 5; 	
  @3 = @2 + T(q/128) % 3;
  @4 = (q/16) % 5;
  @5 = (q/512);
  @6 = (q/16)%4;

  o = (t>>@6) * @5 * 2;
  a = t*(@5+@0) >> @1;
  b = t - @2*128;
  c = b*64 | b*@3>>@4;
  
  p = (o | a) ^ c | p<<12;      
  z = @5/8 % 2;
  s = ((t/4 % w) * @5) / 32 * (q%16<8);

  [0] = (w/2 + p<<z) | (o*z) | s;
  [1] = (w/2 - p>>z) | (o*z) | s;
      ]]>
    </code>
