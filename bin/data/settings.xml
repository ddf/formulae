  <program name="prime_glitch" bits="15" tempo="60" memory="512">
    <code>
      <![CDATA[
  c = 16 * (1 + (q/1024)%8) + (q/(256 - 16*V0))%16;
  r = q / c;
  a = t / (32 - V4);
  
  s = $(q / 128) * (a << (r % 3 + 1) & a << (r % 5 + 1));
  z = (s << 3) ^ (s << 5) ^ (s << 7) ^ (s << 11) ^ (s << 13) ^ (s << 17) ^ (s << 19) ^ (s << 23);
  z = q < 128 * 4 | V4 < 1 ? 0 : z;
  
  j = j + (64 - 4 * (q % 16));
  k = $(j) << 4 >> (q*V3); k = k * (V3>0);
  
  s = ((q / 64) % 2) * ($(t * 320) | R(w / 2)) << 6 >> (q*V2);
  s = s * (V2 > 0);
  
  b = V1;
  h = ((w / 4) + R(w / 2)) >> (q*(c < 112 ? b : b-1));
  h = h * (V1 > 0);
  
  d = (k | s | h) * (q / 1024 + 1);
  
  z = z >> d * d;
  d = d >> c / 32;
  
  [0] = d | (w / 2 + z);
  [1] = d | (w / 2 - z);
      ]]>
    </code>
