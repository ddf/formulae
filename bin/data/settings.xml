<?xml version="1.0"?>
<root>
  <program name="prime_glitch" bits="15" tempo="60" memory="512">
    <code>
      <![CDATA[
	  c = 16 * (1 + (q/1024)%8) + (q/(256 - 16*V0))%16;
		r = q / c;
		a = t / (32 - V4);
		s = $(q / 128) * (a << (r % 3 + 1) & a << (r % 5 + 1));
		z = (s << 3) ^ (s << 5) ^ (s << 7) ^ (s << 11) ^ (s << 13) ^ (s << 17) ^ (s << 19) ^ (s << 23);
		z = q < 128 * 4 | V4 < 1 ? 0 : z;
		j = j + (64 - 4 * (q % 16));
		k = $(j) << 4 >> (q*V3); k = k * (V3>0);
		s = ((q / 64) % 2) * ($(t * 320) | R(w / 2)) << 6 >> (q*V2);
		s = s * (V2 > 0);
		b = V1;
		h = ((w / 4) + R(w / 2)) >> (q*(c < 112 ? b : b-1));
		h = h * (V1 > 0);
		d = (k | s | h) * (q / 1024 + 1);
		z = z >> d * d;
		d = d >> c / 32;
		[0] = d | (w / 2 + z);
		[1] = d | (w / 2 - z);
    ]]>
    </code>
    <interface w="640" h="380">
      
      <control type="slider" name="h" target="V1" min="0" max="32" value="4">
        <shape x="10" y="20" w="100" h="10"/>
      </control>
      <viz name="hviz" source="h" type="scatter" buffer="735">
        <shape x="10" y="31" w="100" h="56"/>
      </viz>

      <control type="slider" name="s" target="V2" min="0" max="32" value="1">
        <shape x="10" y="100" w="100" h="10"/>
      </control>
      <viz name="sviz" source="s" type="bars" buffer="8192">
        <shape x="10" y="111" w="100" h="56"/>
      </viz>

      <control type="slider" name="k" target="V3" min="0" max="8" value="1">
        <shape x="10" y="180" w="100" h="10"/>
      </control>
      <viz name="kviz" source="k" type="bars" buffer="8192">
        <shape x="10" y="191" w="100" h="56"/>
      </viz>

      <control type="slider" name="c" target="V0" min="0" max="15" value="8">
        <shape x="10" y="260" w="100" h="10"/>
      </control>
      <viz name="cviz" source="c" type="blocks" buffer="1" range="128">
        <shape x="10" y="271" w="100" h="56"/>
      </viz>

      <control type="slider" name="z" target="V4" min="0" max="31" value="31">
        <shape x="120" y="20" w="510" h="10"/>
      </control>
      <viz name="zviz" source="z" type="bars" buffer="128" range="32768">
        <shape x="120" y="31" w="510" h="216"/>
      </viz>

    </interface>
  </program>

  <program name="uncertain_cycles" bits="8" tempo="120" memory="512">
    <code>
      <![CDATA[
        v = 8*V0;
        f = (f + (q/v)) % (2*(p+1));
        a = ((t*f) - $(t * (f/2)));
        v = V1;
        b = t << (a % (v * w)) / w;
        v = 4*V3;
        c = (#(t/2) << q%v) % w;
        s = (a | b ^ (a ^ ~b) & c);
        v = 16*V4;      
        p = p + (q%v < 1 & r<q ? q : 0);
        p = p % 11;
        r = q;
        [0] = s << p/2;
        [1] = ~s >> p/2;
        ]]>
    </code>
    <interface w="640" h="380">
      
      <control type="slider" name="p" target="V4" min="1" max="32" value="16">
        <shape x="10" y="20" w="100" h="10"/>
      </control>
      <viz name="pviz" source="p" type="blocks" buffer="1" range="11" columns="2">
        <shape x="10" y="31" w="100" h="56"/>
      </viz>
      
      <control type="slider" name="a" target="V0" min="1" max="16" value="16">
        <shape x="120" y="20" w="500" h="10"/>
      </control>
      <viz name="aviz" source="a" type="bars" buffer="256" range="256">
        <shape x="120" y="31" w="500" h="90"/>
      </viz>
  
      <control type="slider" name="b" target="V1" min="1" max="32" value="8">
        <shape x="120" y="134" w="500" h="10"/>
      </control>
      <viz name="bviz" source="b" type="wave" buffer="128" rate="16" range="256">
        <shape x="120" y="145" w="500" h="90"/>
      </viz>

      <control type="slider" name="c" target="V3" min="1" max="16" value="8">
        <shape x="120" y="248" w="500" h="10"/>
      </control>
      <viz name="cviz" source="c" type="bars" buffer="2048">
        <shape x="120" y="259" w="500" h="90"/>
      </viz>
      
    </interface>
  </program>

  <program name="fibowave" bits="8" tempo="144" memory="8192">
    <code>
      <![CDATA[
      @0 = 0; @1 = 1; r = 1024*8;
      @t = (t > 1 & t < r) ? @(t-1) + @(t-2) : @t;
      
      v = V0 * 32;
      a = v;  i = (q/a) % r;  j = q%a;  n = 64;
      b = @i%n + (@(i+1)%n - @i%n) * j / a;
            
      o = o + F(24 + b) / 32;
      s = (o >> 4);
      s = s | $(o)*(i%2);
      v = 128 - 8*V1;
      s = V1 > 0 ? s / v * v : 0;
      
      v = V2+1;
      f = (q/128) % v;
      c = 1 + (q/(1024*4)) % (f+4);
      d = (d = (q/128) % 8) > 6 ? q*c : (d < 4 ? q/(2<<c) : q/(1<<c));      
      k = $(t/2*(f+1)) << 2 >> d;
      k = V2 > 0 ? k : 0;
      
      v = 36 - V3*4;
      r = q + 128;
      h = V3 > 0 ? (#Rw << 2 >> (r/v)%(v/4) >> q%(v*8)) : 0;
      
      z = (s & (q & 0xFF)) >> (k/5);
      
      [0] = (w/2 + z) ^ h | k;
      [1] = (w/2 + ~z) ^ h | k;
      ]]>
    </code>
    <interface w="640" h="380">
      <control type="slider" name="b" target="V0" min="1" max="16" value="8">
        <shape x="10" y="20" w="600" h="10"/>
      </control>
      <viz name="bviz" source="b" type="bars" buffer="128" rate="64" range="128">
        <shape x="10" y="31" w="600" h="56"/>
      </viz>

      <control type="slider" name="s" target="V1" min="0" max="15" value="10">
        <shape x="10" y="100" w="600" h="10"/>
      </control>
      <viz name="sviz" source="s" type="bars" buffer="256" range="256">
        <shape x="10" y="111" w="600" h="56"/>
      </viz>

      <control type="slider" name="k" target="V2" min="0" max="8" value="1">
        <shape x="10" y="180" w="250" h="10"/>
      </control>
      <viz name="kviz" source="k" type="bars" buffer="4096">
        <shape x="10" y="191" w="250" h="56"/>
      </viz>

      <control type="slider" name="h" target="V3" min="0" max="4" value="1">
        <shape x="330" y="180" w="250" h="10"/>
      </control>
      <viz name="hviz" source="h" type="bars" buffer="8192">
        <shape x="330" y="191" w="250" h="56"/>
      </viz>
    </interface>
  </program>

  <program name="butter_beat" bits="8" tempo="15" memory="64">
    <code>
      <![CDATA[
      v = V1*256;
      c = (q/v)%7 + 1;
      m = (q/64);
      
      j = j + $(m*F7) - $(m*F5);
      f = 16 >> c%5;
      h = $(j*f)%w << 1 >> (q*c);
           
      c = (9 - c) / 4;
      u = V3*4 << c;
      v = V3*3 << c;
      a = $(j/4) << 1 >> (q%u);
      b = $(j/2) << 1 >> (q%v);
      k = a | b;
            
      u = V2*6 << c;
      v = V2*5 << c;
      a = $(j/4) << 1 >> (q%u);
      b = $(j/2) << 1 >> (q%v);
      s = a | b;                 
            
      d = $(j/8 + $(j/2048)) >> 2;
      d = d | h;
      
      [0] = w/2 + (d | k)/4;
      [1] = w/2 + (d | s)/4;
      ]]>
    </code>

    <interface w="640" h="360">

      <control type="slider" name="k" target="V3" min="1" max="8" value="8">
        <shape x="60" y="15" w="100" h="10"/>
      </control>
      <viz name="kviz" source="k" type="bars" buffer="256" rate="8">
        <shape x="60" y="26" w="100" h="325"/>
      </viz>

      <control type="slider" name="h" target="V1" min="1" max="8" value="4">
        <shape x="270" y="15" w="100" h="10"/>
      </control>
      <viz name="hviz" source="h" type="bars" buffer="256" rate="8">
        <shape x="270" y="26" w="100" h="325"/>
      </viz>

      <control type="slider" name="s" target="V2" min="1" max="8" value="8">
        <shape x="480" y="15" w="100" h="10"/>
      </control>
      <viz name="sviz" source="s" type="bars" buffer="256" rate="8">
        <shape x="480" y="26" w="100" h="325"/>
      </viz>
      
    </interface>
    
  </program>
</root>


