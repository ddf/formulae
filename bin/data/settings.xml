  <program name="improper/fractions" bits="8" tempo="15" memory="64">
    <code>
      <![CDATA[
  t==0 ? i = 64;
  
  v = V1*256;
  c = (q/v)%7 + 1; 
  m = (q/(16 + i%64)) + 1;
  z = w;
      
  j = j + $(m*F7) - $(m*F5);
  f = 16 >> c%5;
  h = $(j*f)%w << 1 >> (q*c);
      
  h > z ? i = i + 1;
           
  c = (9 - c) / 4;
  u = V3*4 << c;
  v = V3*3 << c;
  a = $(j/4) << 1 >> (q%u);
  b = $(j/2) << 1 >> (q%v);
  k = a | b;
      
  a > z ? i = i + 4;
  b > z ? i = i + 3;
            
  u = V2*6 << c;
  v = V2*5 << c;
  a = $(j/4) << 1 >> (q%u);
  b = $(j/2) << 1 >> (q%v);
  s = a | b;                 
      
  a > z ? i = i + 6;
  b > z ? i = i + 5;
            
  d = $(j/8 + $(j/2048)) * 3 / 4;
  d = d | h;
      
  [0] = w/2 + (d | k)/4;
  [1] = w/2 + (d | s)/4;
      ]]>
    </code>

