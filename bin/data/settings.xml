  <program name="uncertain_cycles" bits="8" tempo="120" memory="512">
    <code>
      <![CDATA[
  v = 8*V0;
  f = (f + (q/v)) % (2*(p+1));
  a = ((t*f) - $(t * (f/2)));
  
  v = V1;
  b = t << (a % (v * w)) / w;
  
  v = 4*V3;
  c = (#(t/2) << q%v) % w;
  
  s = (a | b ^ (a ^ ~b) & c);
  
  v = 16*V4;      
  p = p + (q%v ? 0 : 1);
  p = p % 7;
  
  [0] = s << p/2;
  [1] = ~s >> p/2;
      ]]>
    </code>
