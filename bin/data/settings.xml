  <program name="fibowave" bits="8" tempo="144" memory="8192">
    <code>
      <![CDATA[
  @0 = 0; @1 = 1; r = 1024*8;
  (t > 1 & t < r) ? @t = @(t-1) + @(t-2);
      
  v = V0 * 32;
  a = v;  i = (q/a) % r;  j = q%a;  n = 64;
  b = @i%n + (@(i+1)%n - @i%n) * j / a;
            
  o = o + F(24 + b) / 32;
  s = (o >> 4);
  s = s | $(o)*(i%2);
  v = 128 - 8*V1;
  s = V1 > 0 ? s / v * v;
      
  v = V2+1;
  f = (q/128) % v;
  c = 1 + (q/(1024*4)) % (f+4);
  d = (d = (q/128) % 8) > 6 ? q*c : (d < 4 ? q/(2<<c) : q/(1<<c));      
  k = $(t/2*(f+1)) << 2 >> d;
  k = V2 > 0 ? k;
      
  v = 36 - V3*4;
  r = q + 128;
  h = V3 > 0 ? (#Rw << 2 >> (r/v)%(v/4) >> q%(v*8));
      
  z = (s & (q & 0xFF)) >> (k/5);
      
  [0] = (w/2 + z) ^ h | k;
  [1] = (w/2 + ~z) ^ h | k;
      ]]>
    </code>
